{
  "expo": {
    "name": "Gym World",
    "slug": "gym_world",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/client/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/client/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/client/assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/client/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "student-9tl"
        }
      ]
    ]
  }
}
